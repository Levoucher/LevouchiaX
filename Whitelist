local formatNumber = (function(n)
    n = tostring(n)
    return n:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
end)

local function checkswag()
    for _, v in ipairs(game:GetService('Workspace').Players:GetChildren()) do
        if v:FindFirstChild('UpperTorso') and not v:FindFirstChild('UpperTorso'):FindFirstChild('OriginalSize') then
            local plrcheck = game:GetService('Players'):FindFirstChild(v.Name)
            if plrcheck then
                local plrID = plrcheck.UserId
                local displayName = game.Players[v.Name].DisplayName

                if v:FindFirstChildWhichIsA('Humanoid') then
                    if plrID == 3158763149 or plrID == 2859615602  or plrID == 3096529992 then
                        displayName = ('[ü™ê]' .. displayName) 
                    elseif plrID == 1260278612 then
                        displayName = ('evilü§ë') 
                    elseif table.find(ADMINS, plrID) then
                        displayName = ('[üíé]' .. displayName)
                    elseif table.find(MODS, plrID) then
                        displayName = ('[üçÜ]' .. displayName)
                    elseif table.find(CUSTOMWINNER, plrID) then
                        displayName = ('[üò≥]' .. displayName)
                    elseif table.find(NOOBEMOJI, plrID) then
                        displayName = ('[ü•±üò¥]' .. displayName)
                    elseif table.find(STARS, plrID) then
                        displayName = ('[‚≠ê]' .. displayName)
                    elseif plrID == 1997397874 then
                        displayName = ('[ü¶çüëëüêíü§¨ü§©‚≠ê] kill if gay]' .. displayName)
                    else
                        displayName = ('[üòé]' .. displayName)
                    end

                    v:FindFirstChildWhichIsA('Humanoid').DisplayName = displayName
                end
            end
        end
    end
end
                local plrcheck = game.Players:FindFirstChild(v.Name)
                if plrcheck then
                    local plrID = game.Players:FindFirstChild(v.Name).UserId
                    if plrID == 969017656 or plrID == 200547759 then
                        if v:FindFirstChildWhichIsA('Humanoid') then           
                            v:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üí†]' .. game.Players[v.Name].DisplayName) 
                        end
                    elseif table.find(ADMINS, plrID) then
                        if v:FindFirstChildWhichIsA('Humanoid') then
                            v:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üíé]' .. game.Players[v.Name].DisplayName)
                        end
                    elseif table.find(MODS, plrID) then
                        if v:FindFirstChildWhichIsA('Humanoid') then
                            v:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üçÜ]' .. game.Players[v.Name].DisplayName)
                        end
           
                    elseif table.find(STARS, plrID) then
                        if v:FindFirstChildWhichIsA('Humanoid') then
                            v:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚≠ê]' .. game.Players[v.Name].DisplayName)
                        end
                    end
                end

local succ, errr = pcall(checkswag)


